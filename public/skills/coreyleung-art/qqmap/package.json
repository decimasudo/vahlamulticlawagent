{
  "name": "tencent-map",
  "version": "1.0.0",
  "description": "腾讯地图Web服务API集成，用于地点搜索、路线规划、逆地理编码等功能",
  "author": "OpenClaw Community",
  "dependencies": {
    "system": ["curl", "python3"],
    "env": ["TENCENT_MAP_KEY"]
  },
  "entryPoints": {
    "main": "./scripts/tencent_map.sh"
  },
  "permissions": {
    "network": true,
    "env": ["TENCENT_MAP_KEY"]
  },
  "triggers": [
    "腾讯地图",
    "qq地图",
    "位置搜索",
    "地点查询",
    "地图搜索"
  ]
}