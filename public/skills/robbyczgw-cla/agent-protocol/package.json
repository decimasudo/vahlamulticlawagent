  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/robbyczgw-cla/agent-protocol.git"
  },
  "homepage": "https://github.com/robbyczgw-cla/agent-protocol",
  "bugs": {
    "url": "https://github.com/robbyczgw-cla/agent-protocol/issues"
  },
  "main": "scripts/protocol.js",
